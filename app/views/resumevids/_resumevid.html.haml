= div_for resumevid do
  %li
    %b Name:
    =resumevid.name
    %b Embedded Link:
    =resumevid.originvid.embedded
    %b Original Link(prior to translating to embedded code):
    =resumevid.originvid
    %br
    %iframe{:allowfullscreen => "", :frameborder => "0", :height => "270", :src => resumevid.originvid.embedded, :width => "480"} 
    %br
    - if current_member?(@member)
      = link_to 'Destroy', [resumevid.member, resumevid],
               :confirm => 'Are you sure that you want to delete?',
               :method=> :delete,
               :class=>"delete",
               :remote => true
    
    %br
  %hr
